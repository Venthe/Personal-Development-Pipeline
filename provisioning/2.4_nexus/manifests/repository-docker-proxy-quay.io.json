{
  "dockerProxy": {
    "indexType": "HUB",
    "indexUrl": "https://quay.io"
  },
  "proxy": {
    "contentMaxAge": 1440,
    "metadataMaxAge": 1440,
    "remoteUrl": "https://quay.io"
  },
  "httpClient": {
    "blocked": false,
    "autoBlock": true
  },
  "name": "docker-proxy-quay.io",
  "online": true,
  "storage": {
    "blobStoreName": "docker-default",
    "strictContentTypeValidation": true,
    "writePolicy": "allow"
  },
  "docker": {
    "v1Enabled": false,
    "forceBasicAuth": false
  },
  "negativeCache": {
    "enabled": false,
    "timeToLive": 1440
  }
}
