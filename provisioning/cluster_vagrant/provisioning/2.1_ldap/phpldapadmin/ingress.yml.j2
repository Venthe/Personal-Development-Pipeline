
apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: phpldapadmin-ingress
  namespace: {{ namespace }}
  annotations:
    kubernetes.io/ingress.class: "nginx"
spec:
  rules:
  - host: "ldap-admin.{{ ldap_domain }}"
    http:
      paths:
      - backend:
          service:
            name: phpldapadmin
            port:
              number: 80
        pathType: ImplementationSpecific